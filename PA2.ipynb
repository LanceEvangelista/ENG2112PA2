    "import numpy as np\n",
    "\n",
    "#generate a 10 x 10 nd Array\n",
    "\n",
    "intarray = np.arange(1, 101) ** 2\n",
    "\n",
    "for x in intarray:\n",
    "    divisible = intarray[intarray % 3 == 0]\n",
    "    \n",
    "print(intarray)\n",
    "\n",
    "#save as .npy\n",
    "\n",
    "np.save(\"div_by_3.npy\", divisible)"
